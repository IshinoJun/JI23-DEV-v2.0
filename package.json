{
  "name": "JI23-DEV-v2.0",
  "version": "1.5.0",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida --output src/lib --enableStatic --ignorePath .gitignore --watch --appDir",
    "build": "aspida && pathpida --output src/lib --enableStatic --ignorePath .gitignore && next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint-staged": "lint-staged",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "analyze": "ANALYZE=true npm run build",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@next/bundle-analyzer": "^12.3.0",
    "axios": "^0.27.2",
    "bright": "^0.8.4",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "html-react-parser": "^5.0.6",
    "microcms-js-sdk": "^2.2.1",
    "next": "^13.5.2",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "tailwindcss-animation-delay": "^1.0.7",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/gtag.js": "^0.0.14",
    "@types/node": "^18.17.19",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.2",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.22.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2"
  }
}
