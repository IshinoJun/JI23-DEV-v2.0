{
  "name": "JI23-DEV-v2.0",
  "version": "1.2.0",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida --output src/lib --enableStatic --ignorePath .gitignore --watch",
    "build": "aspida && pathpida --output src/lib --enableStatic --ignorePath .gitignore && next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint-staged": "lint-staged",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "analyze": "ANALYZE=true npm run build",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run build",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@next/bundle-analyzer": "^12.3.0",
    "axios": "^0.27.2",
    "canvas": "^2.10.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "microcms-js-sdk": "^2.2.1",
    "next": "^13.5.2",
    "next-seo": "^6.1.0",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "rehype-highlight": "^5.0.2",
    "simplebar-react": "^2.4.1",
    "tailwindcss-animation-delay": "^1.0.7",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.2",
    "@emotion/jest": "^11.10.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/gtag.js": "^0.0.14",
    "@types/node": "18.7.18",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "babel-jest": "^29.0.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.2",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.22.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.1.8",
    "twin.macro": "^3.4.0",
    "typescript": "5.2"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
